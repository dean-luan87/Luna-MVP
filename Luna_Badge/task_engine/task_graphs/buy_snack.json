  "nodes": [
    {
      "id": "find_store",
      "type": "observation",
      "title": "寻找商店",
      "description": "识别附近的商店",
      "config": {
        "type": "ocr",
        "target": "store_signs"
      },
      "status": "pending",
      "timeout": 120
    },
    {
      "id": "navigate_to_store",
      "type": "navigation",
